{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build_cpp",
      "type": "shell",
      "command": "/usr/bin/g++",
      "args": [
        "-std=gnu++17",
        "-Wall",
        "-Wextra",
        "-g",
        "-O0",
        "-DONLINE_JUDGE",
        "-I/opt/boost/gcc/include",
        "-L/opt/boost/gcc/lib",
        "-I${workspaceFolder}/ac-library",
        "-o",
        "'${workspaceFolder}/problem.exe'",
        "'${file}'",
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}
